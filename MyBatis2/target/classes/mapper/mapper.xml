<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="test_db">
 <insert id="insert_data" parameterType="kr.hs.study.model.dto.LoginDTO">
 	insert into login(user_id, user_pass) values(#{user_id},#{user_pass})
 </insert>
 <delete id="insert_data" parameterType="kr.hs.study.model.dto.LoginDTO">
 	DELETE FROM LOGIN WHERE user_id=#{user_id}
 </delete>
</mapper>       